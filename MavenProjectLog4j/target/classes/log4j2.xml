<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <File name="FileLogger" fileName="D://app.log" append="true">
      <PatternLayout pattern="[%d{HH:mm:ss}] %-6level %logger - %msg%n"/>
    </File>
  </Appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="FileLogger"/>
    </Root>
  </Loggers>
</Configuration>